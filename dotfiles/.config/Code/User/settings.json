{
    "window.restoreWindows": "none",
    "window.openFoldersInNewWindow": "off",
    "window.zoomLevel": -0.3,
    "window.density.editorTabHeight": "compact",
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "Monokai",
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f",
        "statusBar.foreground": "#fff",
        "statusBar.debuggingForeground": "#fff"
    },
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.stickyScroll.enabled": false,
    "editor.hover.sticky": false,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 11,
    "editor.fontVariations": true,
    "editor.wordWrap": "off",
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "terminal.integrated.showExitAlert": false,
    "vim.hlsearch": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#005f5f",
        "#fff"
    ],
    "vim.statusBarColors.insert": [
        "#5f0000",
        "#fff"
    ],
    "vim.statusBarColors.visual": [
        "#5f00af",
        "#fff"
    ],
    "vim.statusBarColors.visualline": [
        "#005f87",
        "#fff"
    ],
    "vim.statusBarColors.visualblock": [
        "#86592d",
        "#fff"
    ],
    "vim.statusBarColors.replace": [
        "#000000",
        "#fff"
    ],
    "vim.statusBarColors.commandlineinprogress": [
        "#86592d",
        "#fff"
    ],
    "vim.statusBarColors.searchinprogressmode": [
        "#86592d",
        "#fff"
    ],
    "vim.statusBarColors.easymotionmode": [
        "#86592d",
        "#fff"
    ],
    "vim.statusBarColors.easymotioninputmode": [
        "#86592d",
        "#fff"
    ],
    "vim.statusBarColors.surroundinputmode": [
        "#86592d",
        "#fff"
    ],
    "vim.enableNeovim": true,
    "vim.commandLineModeKeyBindings": [],
    "vim.handleKeys": {
        "<C-b>": false,
        "<C-d>": false,
        "<C-n>": false,
        // "<C-w>": false,
        "<C-c>": false,
        "<C-v>": false,
        "<C-x>": false,
        "<C-f>": false,
        "<C-p>": false,
        "<C-t>": false
    },
    "vim.leader": "<space>",
    "vim.surround": true,
    "vim.smartRelativeLine": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#a9dc7660",
    "vim.highlightedyank.duration": 250,
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "r"
            ],
            "commands": [
                "renameFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "b"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "q",
            ],
            "commands": [
                "workbench.action.closeActiveEditor",
            ]
        },
        {
            "before": [
                "Q",
            ],
            "commands": [
                "workbench.action.closeEditorsInGroup",
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.codeAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "e"
            ],
            "commands": [
                "workbench.actions.view.problems"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "g"
            ],
            "commands": [
                "workbench.scm.focus"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditorDown"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditorLeft"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        }
    ],
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "breadcrumbs.enabled": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "git.autofetch": true,
    "git.confirmSync": false,
    "redhat.telemetry.enabled": false,
    "debug.console.fontSize": 12,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "git.openRepositoryInParentFolders": "always",
    "calva.prettyPrintingOptions": {
        "printEngine": "pprint",
        "enabled": true,
        "width": 120,
        "maxLength": 50
    },
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "window.menuBarVisibility": "toggle",
    "workbench.tree.enableStickyScroll": false,
}